<Window x:Class="UBB_SE_2024_Team_42.GUI.SettingsPop_Up"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UBB_SE_2024_Team_42.GUI"
        mc:Ignorable="d"
        Title="SettingsPop_Up" Height="550" Width="800">
    <Grid Margin="0,0,0,0" Background="#FF625ED7">
        <TextBlock Text="Settings" Margin="41,10,511,456" FontSize="50" FontWeight="Bold"></TextBlock>
        <Button Margin="41,130,104,329" Background="Black" Foreground="#FFF7F7F7">
            <DockPanel>
                <Image/>
                <TextBlock Text="EDIT" FontSize="40" HorizontalAlignment="Left" VerticalAlignment="Center" FontWeight="Bold"/>
            </DockPanel>
        </Button>
        <Button Margin="40,230,104,229" Foreground="White" Background="#FF6C06AA">
            <DockPanel>
                <Image/>
                <TextBlock Text="SAVE" FontSize="40" FontWeight="Bold"/>
            </DockPanel>
        </Button>
        <Button Margin="40,338,105,118" Background="#FF6C06AA" Foreground="White">
            <DockPanel>
                <Image/>
                <TextBlock FontSize="40" Text="DELETE" FontWeight="Bold"/>
            </DockPanel>
        </Button>
    </Grid>
</Window>
